  ],
  "author": "Jiang Fengming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jiangfengming/what-browser/issues"
  },
  "homepage": "https://github.com/jiangfengming/what-browser#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-config-enough": "^0.3.4",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
